{
  "name": "cms",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/cms/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/cms",
        "command": "npm run develop"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "cwd": "apps/cms",
        "commands": [
          "NODE_ENV=production npm run build",
          "rm -rf ../../dist/apps/cms",
          "mkdir -p ../../dist/apps",
          "mv dist ../../dist/apps/cms",
          "cp -r public ../../dist/apps/cms",
          "cp favicon.png ../../dist/apps/cms",
          "cp package.json ../../dist/apps/cms",
          "cp package-lock.json ../../dist/apps/cms",
          "cd ../../dist/apps/cms && npm install --omit=dev"
        ],
        "parallel": false,
        "outputPath": "dist/apps/cms"
      }
    },
    "generate-types": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/cms",
        "command": "node_modules/.bin/strapi ts:generate-types -o ../../libs/survey-model/src/lib/generated -f strapi-schema.ts"
      }
    }
  },
  "tags": []
}
